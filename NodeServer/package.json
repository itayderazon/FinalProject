{
    "name": "nutrition-server-postgresql",
    "version": "1.0.0",
    "description": "Node.js server with PostgreSQL for nutrition tracking",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "test": "jest",
        "test:watch": "jest --watch",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "seed": "cd ../database && npm run seed",
        "check-db": "cd ../database && npm run check-connection",
        "db-stats": "cd ../database && npm run validate",
        "reset-db": "cd ../database && npm run reset-db && npm run seed"
    },
    "keywords": [
        "nutrition",
        "nodejs",
        "express",
        "mvc",
        "api",
        "postgresql",
        "jwt"
    ],
    "author": "Your Name",
    "license": "MIT",
    "dependencies": {
        "axios": "^1.7.7",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "express-rate-limit": "^7.4.1",
        "express-validator": "^7.2.0",
        "helmet": "^8.0.0",
        "joi": "^17.9.2",
        "jsonwebtoken": "^9.0.2",
        "multer": "^1.4.5-lts.1",
        "pg": "^8.16.0",
        "winston": "^3.15.0"
    },
    "devDependencies": {
        "@types/pg": "^8.10.7",
        "eslint": "^9.13.0",
        "jest": "^29.7.0",
        "nodemon": "^3.1.7",
        "supertest": "^7.0.0"
    },
    "engines": {
        "node": ">=16.0.0",
        "npm": ">=8.0.0"
    }
}
